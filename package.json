{
  "name": "gti-helpdesk-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "prisma generate && tsx scripts/check-db.ts && prisma db push && tsx scripts/seed-default-user.ts",
    "dev": "next dev",
    "dev:watch": "concurrently \"npm run docker:logs\" \"npm run dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down -v",
    "docker:logs": "docker compose logs -f mariadb",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "next": "16.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "prisma": "^6.18.0",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
